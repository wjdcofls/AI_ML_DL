    "# Îã§Ìï≠ ÌöåÍ∑Ä\n",
    "x = car['# \"speed\"'].values.reshape(-1,1)      # (400,) --> (400,1),2Ï∞®Ïõê, -1ÏùÄ ÏûêÎèô Í≥ÑÏÇ∞\n",
    "y = car['dist'].values.reshape(-1,1)    # (400,) --> (400,1),2Ï∞®Ïõê, -1ÏùÄ ÏûêÎèô Í≥ÑÏÇ∞\n",
