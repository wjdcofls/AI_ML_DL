    "# Ïã§Ïäµ Í≥ºÏ†ú Boston\n",
    "\n",
    "import tensorflow as tf\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "tf.random.set_seed(5)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "c7c691d8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
