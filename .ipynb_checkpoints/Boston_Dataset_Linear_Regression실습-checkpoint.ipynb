      "(400, 9) (400, 1)\n"
     ]
    }
   ],
   "source": [
    "# Îç∞Ïù¥ÌÑ∞ Î∂àÎü¨Ïò§Í∏∞\n",
    "xy = np.loadtxt('boston_train.csv', delimiter=',', skiprows=1, dtype=np.float32)\n",
    "\n",
    "x_train = xy[:,:-1]     # x\n",
    "y_train = xy[:,[-1]]    # y\n",
    "print(x_train.shape, y_train.shape)  # (400, 9) (400, 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "4e751371",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Î≥ÄÏàò Ï¥àÍ∏∞Ìôî : weight, bias\n",
    "# (m,n)  *  (n.l) = (m,l) : ÌñâÎ†¨Ïùò ÎÇ¥Ï†Å Í≥±ÏÖà Í≥µÏãù\n",
    "#   X    *   W    =  Y\n",
    "# (400, 9) * (9,1)  = (400,1)\n",
    "W = tf.Variable(tf.random.normal([9,1]), name='weight')\n",
    "b = tf.Variable(tf.random.normal([1]), name='bias')\n",
    "# W,b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "401ae8a8",
   "metadata": {},
   "outputs": [],
   "source": [
    "# ÏòàÏ∏° Ìï®Ïàò (hypothesis) : H(x) = X1*W1 + X2*W2 + X3*W3 + b\n",
    "def hypothesis(X):\n",
    "    return tf.matmul(X,W) + b  # ÎÇ¥Ï†Å Í≥±ÏÖà"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "2b6dc1b9",
   "metadata": {},
   "outputs": [],
   "source": [
    "# ÎπÑÏö© Ìï®Ïàò : (H(X) - Y)^2 Ïùò ÌèâÍ∑†\n",
    "# tf.square() : Ï†úÍ≥±\n",
    "# tf.reduce_mean() : ÌèâÍ∑†\n",
    "\n",
    "def cost_func():\n",
    "    cost = tf.reduce_mean(tf.square(hypothesis(x_train) - y_train))\n",
    "    return cost"
   ]
  },
