    "# ÌèâÍ∞Ä : RMSE\n",
    "def get_rmse(y_test,preds):\n",
    "    squared_error = 0\n",
    "    for k,_ in enumerate(y_test):\n",
    "        squared_error += (preds[k] - y_test[k])**2\n",
    "    mse = squared_error/len(y_test)  \n",
    "    rmse = np.sqrt(mse)\n",
    "    return rmse[0]\n",
    "\n",
