    "# ÎπÑÏö© Ìï®ÏàòÏùò Íµ¨ÌòÑ\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def cost(x,y,w) : \n",
    "    c = 0\n",
    "    for k in range(len(x)) :    # 3Ìöå Î∞òÎ≥µ\n",
    "        hx = w*x[k]             # ÏòàÏ∏° Ìï®Ïàò\n",
    "        loss = (hx - y[k])**2   # (ÏòàÏ∏°Í∞í - Ïã§Ï†úÍ∞í)^2   / ÎπÑÏö©Ìï®Ïàò\n",
    "        c += loss\n",
    "    return c/len(x)             # ÎπÑÏö©Ïùò ÌèâÍ∑†Í∞íÏùÑ Î∞òÌôò\n",
    "    \n",
    "x = [1,2,3]\n",
    "y = [1,2,3]\n",
    "\n",
    "print('w:-1, cost:', cost(x,y,-1))    # hx = [-1,-2,-3], cost: 18.666666666666668\n",
    "print('w:0, cost:', cost(x,y,0))      # hx = [0,0,0],    cost: 4.666666666666667 \n",
    "print('w:1, cost:', cost(x,y,1) )     # hx = [1,2,3],    cost: 0.0\n",
    "print('w:2, cost:', cost(x,y,2) )     # hx = [2,4,6],    cost: 4.666666666666667\n",
    "print('w:3, cost:', cost(x,y,3) )     # hx = [3,6,9],    cost: 18.666666666666668\n",
    "\n",
    "\n",
    "# ÎπÑÏö©Ìï®ÏàòÏùò ÏãúÍ∞ÅÌôî : xÏ∂ïÏùÄ weight, yÏ∂ïÏùÄ costÎ°ú ÌïòÎäî 2Ï∞® Ìï®Ïàò, Ìè¨Î¨ºÏÑ†Ïùò Î∞©Ï†ïÏãù\n",
    "for k in range(-30,50) :\n",
    "    w = k/10\n",
    "    c = cost(x,y,w)\n",
    "    plt.plot(w,c,'o')\n",
